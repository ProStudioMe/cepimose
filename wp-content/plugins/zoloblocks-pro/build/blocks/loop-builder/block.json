{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "title": "Loop Builder",
  "name": "zolo/loop-builder",
  "category": "zoloblocks",
  "keywords": [
    "query",
    "post",
    "post query",
    "query builder",
    "loop builder"
  ],
  "description": "Query your posts",
  "apiVersion": 3,
  "textdomain": "zoloblocks",
  "example": {
    "attributes": {
      "preview": true
    },
    "viewportWidth": 400
  },
  "usesContext": [
    "templateSlug",
    "previewPostType"
  ],
  "supports": {
    "anchor": false,
    "customClassName": false,
    "align": [
      "wide",
      "full"
    ]
  },
  "editorScript": "file:./index.js",
  "style": "file:./style-index.css"
}