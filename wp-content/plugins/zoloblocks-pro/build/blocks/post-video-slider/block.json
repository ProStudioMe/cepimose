{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "title": "Post Video Slider",
  "name": "zolo/post-video-slider",
  "category": "zoloblocks",
  "keywords": [
    "post",
    "post slider",
    "video slider",
    "gallery slider"
  ],
  "description": "Showcase posts in this dynamic carousel slider block",
  "apiVersion": 3,
  "textdomain": "zoloblocks",
  "example": {
    "attributes": {
