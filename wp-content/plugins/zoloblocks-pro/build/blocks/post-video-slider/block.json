    },
    "viewportWidth": 410
  },
  "supports": {
    "anchor": false,
    "customClassName": false,
    "align": [
      "wide",
      "full"
    ]
  },
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": [
    "swiper",
    "file:./style-index.css"
  ],
  "viewScript": "file:./frontend.js",
  "script": [
    "swiper"
  ]
}